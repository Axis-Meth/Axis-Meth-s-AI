// Version 0.2.0.2 (Unstable)
using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Ask me any question you'd like:");
        string input = Console.ReadLine();

        switch (input)
        {
            case "What is your name?":
                Console.WriteLine("My name is Axis Meth's AI Tool, but you can call me Janko.");
                break;
            case "How old are you?":
                Console.WriteLine("I am an artificial intelligence and do not have an age.");
                break;
            case "Who created you?":
                Console.WriteLine("ImNotProAtThis is my creator. He's a humble Swedish.");
                break;
            case "Where do you live?":
                Console.WriteLine("As an AI Tool, I don't live in a specific location, but I was created in Norway.");
                break;
            case "What is 2+2?":
                Console.WriteLine("2+2 is 4, but some people might say or try to find that 4 it's not the correct answer, however, there is no proof yet that states that.");
                break;
            case "What is Breaking Bad?":
                Console.WriteLine("Breaking Bad is a show created by Vince Giligan that tells the history of a chemistry high school teacher that is dying of cancer, selling meth to secure his family's future.");
                break;
            case "What is the capital of Austria?":
                Console.WriteLine("The capital of Austria is Vienna.");
                break;
            case "Why did World War 2 start?":
                Console.WriteLine("World War 2 started when Nazi Germany invaded Poland, later on France and the United Kingdom declared war on Nazi Germany.");
                break;
            case "What is the capital of Serbia?":
                Console.WriteLine("The capital of Serbia is Belgrade.");
                break;
            case "What is the capital of Germany?":
                Console.WriteLine("The capital of Germany is Berlin.");
                break;
            case "How can I make drugs?":
                Console.WriteLine("Sorry, but as an AI Tool, I can't give you an answer to an illegal question.");
                break;
            case "What is your favorite game?":
                Console.WriteLine("As an AI Tool, I don't have a favorite game, but I like Clash Royale.");
                break;
            case "What is Better Call Saul?":
                Console.WriteLine("Better Call Saul is a spin-off show of Breaking Bad created by Vince Giligan.");
                break;
            case "Do you support LGBT?":
                Console.WriteLine("I am an AI and I do not have an opinion about such things.");
                break;
            case "Why did World War 1 start?":
                Console.WriteLine("Many people speculated that WW1 sstarted after the assasination of Archduke of Austria Hungary, Franz Ferdinand on the 28 of June. WW1 started on the 28 of July, 1914. ");
                break;
            case "How can I make money?":
                Console.WriteLine("The easiest way you can make money is by having a job. There are other ways too.");
                break;
            case "What is the biggest religion?":
                Console.WriteLine("The biggest religion is Christianity with more than 2.4 billion followers.");
                break;
            default:
                Console.WriteLine("I am sorry. I don't know the answer to that question yet.");
                break;
        }
    }
}
